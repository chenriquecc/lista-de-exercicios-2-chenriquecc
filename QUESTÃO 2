import java.util.Scanner; 

  

public class Main{ 

      public static void main(String args[]){ 

           

        Scanner scan = new Scanner(System.in); 

      

  

    System.out.print("Digite o x do primeiro ponto: "); 

    double x1 = Double.parseDouble(scan.nextLine()); 

    System.out.print("Digite o y do primeiro ponto: "); 

    double y1 = Double.parseDouble(scan.nextLine()); 

    System.out.print("Digite o z do primeiro ponto: "); 

    double z1 = Double.parseDouble(scan.nextLine()); 

      

    System.out.print("Digite o x do segundo ponto: "); 

    double x2 = Double.parseDouble(scan.nextLine()); 

    System.out.print("Digite o y do segundo ponto: "); 

    double y2 = Double.parseDouble(scan.nextLine()); 

    System.out.print("Digite o z do segundo ponto: "); 

    double z2 = Double.parseDouble(scan.nextLine()); 

     

     

      

  

    double distancia = dist2(x1, y1, x2, y2, z1, z2); 

    System.out.println("Dist√¢ncia entre os dois pontos: " + 

      distancia); 

  } 

    

  public static double dist2(double x1, double y1, 

                                  double x2, double y2, 

                                  double z1, double z2){ 

    double a = x2 - x1; 

    double b = y2 - y1; 

    double c = Math.sqrt(Math.pow(a, 2) + Math.pow(b, 2)); 

    return c; 

  } 

} 
